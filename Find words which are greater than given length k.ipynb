   "source": [
    "# Find words which are greater than given length k"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def GREWRDS(str1,k):\n",
    "    LONG_WORDS = []\n",
    "    for i in str1.split(' '):\n",
    "        if (len(i) > k):\n",
    "            LONG_WORDS.append(i)\n",
    "    return LONG_WORDS"
   ]
  },
